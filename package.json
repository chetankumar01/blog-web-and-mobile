{
  "name": "blogWebAndMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "webpack-dev": "webpack --config ./web-server/webpack.config.js --watch",
    "clean-web": "rimraf web-server/build/*",
    "start-web-server": "nodemon web-server/",
    "web-dev": "rimraf web-server/build/* && concurrently --kill-others \"npm run webpack-dev\" \"npm run start-web-server\"",
    "web-prod": "webpack -p --config ./web-server/webpack.production.config.js && babel-node web-server.js"
  },
  "dependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
