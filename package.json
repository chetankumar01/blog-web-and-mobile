    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "compression": "1.6.2",
    "cors": "2.8.1",
    "es6-promise": "4.0.5",
    "express": "4.14.0",
    "html-webpack-plugin": "2.24.1",
    "isomorphic-fetch": "2.2.1",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-helmet": "3.1.0",
    "react-native": "0.37.0",
    "react-redux": "5.0.0-beta.3",
    "react-router": "4.0.0-alpha.5",
